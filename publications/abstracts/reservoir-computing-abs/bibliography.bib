Publisher: Nature Publishing Group},
	file = {Full Text PDF:/home/dotson/Zotero/storage/84C375EA/Verzelli et al. - 2019 - Echo State Networks with Self-Normalizing Activati.pdf:application/pdf;Snapshot:/home/dotson/Zotero/storage/FN8MAM9T/s41598-019-50158-4.html:text/html}
}

@article{ma_deepr-esn_2020,
